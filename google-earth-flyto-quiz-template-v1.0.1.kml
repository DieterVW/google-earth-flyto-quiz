<!-- Initiele cameraview en clickable titel in zijbalk -->
<Snippet maxLines="0">
</Snippet>
<description><![CDATA[
	<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 3.2 Final//EN">
	<html>
	<body>
      <table width="400" bgcolor="#FFFFFF">
         <tr>
            <td bgcolor="#FFFFFF">
               <center><font face="Arial" size="+2"><b><i>Quiztitel</i></b></font></center>
               <br><font face="Arial" size="+1">
            	<p><br>Wat weet jij van XXXXXXXXX ?</p>
					<br><a href="#1;flyto"><b>Test je kennis!</a><br>
					</font><br>
            </td>
         </tr>
      </table>
   </body>
   </html>
	]]></description>
<!-- initial Lookat, met Placemark id="inleiding" in beeld (die dezelfde description heeft als wanneer in de zijbalk op de titel wordt geclicked) -->	
	<LookAt>
			<longitude>3.86</longitude>
			<latitude>53.18</latitude>
			<altitude>1000</altitude><!-- Important: higher than in boundingbox -->
			<range>1000000</range>
			<tilt>0</tilt>
			<heading>0</heading>
	</LookAt>




